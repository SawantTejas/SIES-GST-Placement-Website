<script type="text/javascript">
  google.charts.load("current", {packages:["corechart"]});
  google.charts.setOnLoadCallback(drawChart);
  function drawChart() {
    var cgpi = parseFloat(<%= cgpi %>)
    var max =10 - cgpi
    console.log(max)
    var data = google.visualization.arrayToDataTable([
      ['CGPI', 'Value'],
      ['CGPI',     cgpi],
      ['Pointer remaining',     max],

    ]);

    var options = {
      pieHole: 0.6,
      pieSliceText: 'none',
      legend: 'none'
    };

    var chart = new google.visualization.PieChart(document.getElementById('donutchart'));
    chart.draw(data, options);
  }
</script>

<script type="text/javascript">
  google.charts.load('current', {'packages':['corechart']});
  google.charts.setOnLoadCallback(drawChart);
  function drawChart() {
    var cgpi1 = parseFloat(<%= cgpi1 %>)
    var cgpi2 = parseFloat(<%= cgpi2 %>)
    var cgpi3 = parseFloat(<%= cgpi3 %>)
    var cgpi4 = parseFloat(<%= cgpi4 %>)
    var cgpi5 = parseFloat(<%= cgpi5 %>)
    var cgpi6 = parseFloat(<%= cgpi6 %>)
    var data = google.visualization.arrayToDataTable([
          ['Semester', 'SGPI'],
          ['Sem I', cgpi1],
          ['Sem II',  cgpi2],
          ['Sem III',  cgpi3],
          ['Sem IV',  cgpi4],
          ['Sem V',  cgpi5],
          ['Sem VI',  cgpi6]
        ]);

    var options = {
          title: 'Semester wise SGPI Graph',
          legend: { position: 'bottom' }
        };

    var chart = new google.visualization.LineChart(document.getElementById('curve_chart'));

    chart.draw(data, options);
  }

      //script for individual marksheet
    function chngview(mila_id){
      var x = document.getElementById(mila_id);
        if (x.style.display == "none") {
          x.style.display = "block";
        }
        else {
          x.style.display = "none";
        }
    }
    var cgpit =  parseFloat(<%= cgpi %>)
    var percent = 7.4*cgpit+12
    document.getElementById("percent").innerHTML = 'Equivalent percentage = '+percent
    </script>

