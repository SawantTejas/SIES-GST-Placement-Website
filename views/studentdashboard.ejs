  <!--Container Main start-->
  <div class="height-100">
    <h1 style="font-size: 60px;">Hello <%= fname %>!</h1><br>

    <div class="container">
      <div class="row">
        <div class="col-md-3 col-sm-6 col-12">
          <div class="card" style="width: 18rem;">
            <div class="card-body">
              <h3 class="card-title">CGPI</h3>

              <h1 class="card-text"><%= cgpi %></h1>
            </div>
          </div>
        </div>
        <div class="col-md-3 col-sm-6 col-12">
          <div class="card" style="width: 18rem;">
            <div class="card-body">
              <h3 class="card-title">Skills</h3>

              <h1 class="card-text"><%= skills %></h1>
            </div>
          </div>
        </div>
        <div class="col-md-3 col-sm-6 col-12">
          <div class="card" style="width: 18rem;">
            <div class="card-body">
              <h3 class="card-title">Achievements</h3>

              <p class="card-text"><%= achievements %></p>
            </div>
          </div>
        </div>
        <div class="col-md-3 col-sm-6 col-12">
          <div class="card" style="width: 18rem;">
            <div class="card-body">
              <h3 class="card-title">Placement Status</h3>

              <h1 class="card-text"> <%= pstatus %></h1>
            </div>
          </div>
        </div>
      </div>
    </div>
    <br><br><br>
    <div class="card" style="margin:0px 50px;">
      <div class="card-body">
        <h2 class="card-title" >Student Details</h2>
        <div class="container" style="font-size: 20px;">
          <div class="row" style="padding: 10px;">
            <div class="col-md-2 col-sm-6 col-12"><img src="img/profile.png" alt=""></div>
            <div class="col-md-10 col-sm-6 col-12">
              <div class="container" style="font-size: 20px;"><div class="row" style="padding: 10px;">
                <div class="col-md-6 col-sm-12"><p class="card-text">Name: <%= fname %> <%= lname %></p></div>
                <div class="col-md-6 col-sm-12"><p class="card-text">PRN: <%= prn %></p></div></div>
              <div class="row" style="padding: 10px;">
                <div class="col-md-6 col-sm-12"><p class="card-text">Branch: <%= department %></p></div>
                <div class="col-md-6 col-sm-12"><p class="card-text">Year: <%= year %></p></div></div>
              <div class="row" style="padding: 10px;">
                <div class="col-md-6 col-sm-12"><p class="card-text">Phone: <%= phone %></p></div>
                <div class="col-md-6 col-sm-12"><p class="card-text">E-mail: <%= email %></p></div></div>
              </div><br>
            </div>
          </div>
        </div><br>
        <a href="formstudent" class="btn btn-primary" style="margin-left: 50px;">Update</a>
      </div>
    </div>
  </div>
