</head>

<body id="body-pd" class="noselect">
  <script src="js/main2.js"></script>
  <header class="header" id="header">
    <div class="header_toggle"> <i class='bx bx-menu' id="header-toggle"></i> </div>
    <div class="header_img"> <img src="img/tprofile.png" alt="profilepic"> </div>
  </header>
  <div class="l-navbar" id="nav-bar">
    <nav class="nav">
      <div> <a href="#" class="nav_logo"> <img src="img/SIES_Spinner.png" width="32" height="32"> <span
            class="nav_logo-name">SIES GST</span> </a>
        <div class="nav_list">
          <a href="tdashboard" class="nav_link"> <i class='bx bx-grid-alt nav_icon'></i> <span
              class="nav_name">Dashboard</span> </a>
          <a href="tplacement" class="nav_link"> <i class="bi bi-star"></i> <span class="nav_name">Placements</span>
          </a>
          <a href="tstudents" class="nav_link active"> <i class="bi bi-person-lines-fill"></i>Students</span> </a>
          <a href="tannouncements" class="nav_link"> <i class='bx bxs-megaphone nav_icon'></i><span
              class="nav_name">Announcements</span> </a>
        </div>
      </div> <a href="logout" class="nav_link"> <i class='bx bx-log-out nav_icon'></i> <span class="nav_name">SignOut</span>
      </a>
    </nav>
  </div>
  <!--Container Main start-->
  <h1 style="font-size: 60px;">Student Details</h1><br>
  <div class="mainbody" style="margin: 0px 50px;">
    <div class="container">
      <div class="row">
        <div class="col-md-4 col-sm-6 col-12">
          <div class="card" style="width: 100%;">
            <div class="card-body">
              <h4 class="card-title">No. of Students placed</h4>

              <h2 class="card-text">300</h2>
            </div>
          </div>
        </div>
        <div class="col-md-4 col-sm-6 col-12">
          <div class="card" style="width: 100%;">
            <div class="card-body">
              <h4 class="card-title">Average Package</h4>

              <h2 class="card-text">5 LPA</h2>
            </div>
          </div>
        </div>
        <div class="col-md-4 col-sm-6 col-12">
          <div class="card" style="width: 100%;">
            <div class="card-body">
              <h4 class="card-title">Top Package Earner</h4>

              <h2 class="card-text">Anandteertha Rao</h2>
            </div>
          </div>
        </div>
      </div>
    </div>
    <br><br>
  </div>

  <h2>Students list</h2>
  <div class="mainbody" style="margin: 0px 50px;">
    <div>
      <div class="container filterform">
        <form action="\tstud" method="POST">
          <div class="row">
            <div class="col-lg-12 card-margin">
                <div class="card search-form">
                    <div class="card-body p-0">
                        <form id="search-form">
                            <div class="row">
                                <div class="col-12">
                                    <div class="row no-gutters">

                                        <div class="col-md-5 col-12 p-0">
                                            <input type="text" placeholder="Student Name" class="form-control" id="search" name="studname">
                                        </div>
                                        <div class="col-md-3 col-12 p-0">
                                          <input type="text" placeholder="PRN" class="form-control" id="search" name="prnsrch">
                                      </div>
                                        <div class="col-md-3 col-10 p-0">
                                          <select class="form-control" id="exampleFormControlSelect1" name="branchsrch">
                                              <option value=''>Branch</option>
                                              <option>CE</option>
                                              <option>EXTC</option>
                                              <option>IT</option>
                                              <option>MECH</option>
                                              <option>PPT</option>
                                          </select>
                                      </div>
                                        <div class="col-md-1 col-2 p-0">
                                            <button type="submit" class="btn btn-base">
                                                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-search"><circle cx="11" cy="11" r="8"></circle><line x1="21" y1="21" x2="16.65" y2="16.65"></line></svg>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>

        </form>
      </div>
    </div>
  </div>
  <div style="margin: 2% 5%;"><br>
    <section id="Studtab">
      <div class="table-responsive">
        <% if(details!=null) { %>
          <table class="table table-striped">
            <thead>
              <tr>
                <th scope="col">#</th>
                <th scope="col">Student Name</th>
                <th scope="col">PRN</th>
                <th scope="col">Branch</th>
                <th scope="col">Organization</th>
                <th scope="col">Stipend</th>
              </tr>
            </thead>
            <tbody>
              <% details.forEach(function(item){ %>
                <tr>
                  <th scope="row">1</th>
                  <td>
                    <%= item.fname%>
                  </td>
                  <td>
                    <%= item.prn%>
                  </td>
                  <td>
                    <%= item.department%>
                  </td>
                  <td>
                    -
                  </td>
                  <td>
                    -
                  </td>
                </tr>
                <% }) %>
            </tbody>
          </table>
          <% } %>
      </div>
    </section>
    <br><br>
  </div>
</body>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js"
  integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>


</html>
