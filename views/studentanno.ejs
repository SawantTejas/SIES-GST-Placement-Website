<!doctype html>
<html lang="en">

<head>
  <!-- Required meta tags -->
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <!-- Bootstrap CSS -->
  <link rel="icon" type="text/css" href="SIES_Spinner.png">
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.5.0/font/bootstrap-icons.css">
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/boxicons@latest/css/boxicons.min.css">
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/js/bootstrap.bundle.min.js">
  <link rel="stylesheet" href="css/new1.css">

  <title>SIES GST | Student Profile</title>
</head>

<body id="body-pd" class = "noselect">
  <script src="js/main2.js"></script>
  <header class="header" id="header">
    <div class="header_toggle"> <i class='bx bx-menu' id="header-toggle"></i> </div>
    <div class="header_img"> <img src="img/profile.png" alt="profilepic"> </div>
  </header>
  <div class="l-navbar" id="nav-bar">
    <nav class="nav">
      <div> <a href="index" class="nav_logo"> <img src="img/SIES_Spinner.png" width="32" height="32"> <span class="nav_logo-name">SIES GST</span> </a>
        <div class="nav_list">
          <a href="dashboard" class="nav_link"> <i class='bx bx-grid-alt nav_icon'></i> <span class="nav_name">Dashboard</span> </a>
          <a href="academics" class="nav_link"> <i class="bi bi-book"></i> <span class="nav_name">Academics & Projects</span> </a>
          <a href="achievements" class="nav_link"> <i class="bi bi-trophy"></i> <span class="nav_name">Achievements</span> </a>
          <a href="placement" class="nav_link"> <i class="bi bi-star"></i> <span class="nav_name">Placements</span> </a>
          <a href="announcements" class="nav_link active"> <i class='bx bxs-megaphone nav_icon'></i><span class="nav_name">Announcements</span> </a>
         </div>
      </div> <a href="logout" class="nav_link"> <i class='bx bx-log-out nav_icon'></i> <span class="nav_name">SignOut</span> </a>
    </nav>
  </div>
 <!--Container Main start-->
  <br>
  <div class="d-flex">
    <div class="col-md-10 col-sm-8 col-8">
      <h1 style="font-size: 60px;">Announcements</h1><br>
    </div>
  </div>
  <div style="margin-left: 5%; margin-right: 5%;">
    <% if(details!=null) { %>
      <% details.forEach(function(item){ %>
    <div class="card" style="width: auto;">
      <div class="card-body">
        <h3 class="card-title"><%= item.annstitle %></h3>
        <h6 class="card-subtitle mb-2 text-muted">Test</h6>
        <p class="card-text"><%= item.annsdescription %></p>
        <a href="#" class="card-link"><%= item.annslink %></a>
      </div>
    </div><br>
    <% }) %>
    <% } %>

    <div class="card" style="width: auto;">
      <div class="card-body">
        <h3 class="card-title">Conduct Aptitude tests for students of your branch</h3>
        <h6 class="card-subtitle mb-2 text-muted">To teachers</h6>
        <p class="card-text"> Head of Department of all branches are required to conduct aptitude test of their
          respective
          Departments.</p>
        <a href="#" class="card-link">Application Link</a>
        <a href="#" class="card-link">Company Info</a>
      </div>
    </div><br>


    <br><br>

</body>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js"
  integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>
</html>
